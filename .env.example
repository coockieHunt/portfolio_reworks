# .env.example
# Copy this file to .env and fill in the values before running the application.

# Base URL for the API
VITE_API_BASE_URL=http://localhost:3001/api
VITE_ASSETS_BASE_URL=http://localhost:3001/assets
# Fallback cache JSON (static file or CDN)
VITE_FALLBACK_CACHE_URL=/fallback/worker_cache.json
# Umami Analytics
VITE_UMAMI_ID=your-umami-id
VITE_UMAMI_SCRIPT_URL=https://your-umami-instance.com/umami.js
# og:image URL for social sharing
VITE_OG_IMAGE_URL=https://your-domain.com/path-to-your-image.png