# .env.example
# Copy this file to .env and fill in the values before running the application.

# Base URL for the API
VITE_API_BASE_URL=http://localhost:3001/api #api url
VITE_ASSETS_BASE_URL=http://localhost:3001/assets #assets url
VITE_SITE_URL=http://localhost:3000 #self url

# Fallback cache JSON (static file or CDN)
VITE_FALLBACK_CACHE_URL=/fallback/worker_cache.json #fallback cache url

# Umami Analytics
VITE_UMAMI_ID=your-umami-id #umami id
VITE_UMAMI_SCRIPT_URL=https://your-umami-instance.com/umami.js #umami script url



